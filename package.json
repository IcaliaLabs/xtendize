{
  "name": "xtendize",
  "version": "0.0.1",
  "description": "Helps us create chrome extension apps",
  "main": "lib/main.js",
  "scripts": {
    "debug-action": "yarn run build && node -r dotenv/config index.js",
    "build": "tsc && ncc build",
    "format": "prettier --write **/*.ts",
